{
  "serilog": {
    "minimumLevel": {
      "default": "Information"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "FileOptions": {
      "Location": "logs",
      "Filename": "zmdb.log"
    }
  },
  "AllowedHosts": "*",
  "PreloadMovies": {
    "PreloadGrains": false,
    "MoviesSource": "FILE",
    "SourcePath": "starter-movies.json"
  },
  "SiloConfiguration": {
    "Persistences": []
  }
}
